

<div class="field">
      <%= f.label :id, I18n.translate('state_component_type_taxes.field_name.id'), :class => 'label' %>
    <span class="value" id="state_component_type_tax_id"><%= @state_component_type_tax.id %></span>
</div>



<div class="field">
    <%= f.label :state, I18n.translate('state_component_type_taxes.field_name.state'), :class => 'label' %>
    <span class="value">
        <%= collection_select( :state_component_type_tax, :state_id, StatesController.list, :id, :nil_to_s, { :include_blank => true }) %>
    </span>
</div>



