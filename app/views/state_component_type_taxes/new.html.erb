<%
    @page_scope = 'state_component_type_taxes.new'
    @deactivated_module = defined?(StateComponentTypeTax.new.deactivated_module)
%>

<h4><%= I18n.translate('header', :scope => @page_scope) %></h4>

<%= form_for(@state_component_type_tax, :action => 'create') do |f| %>

    <%= render :partial => 'layouts/errors_form', :locals => { :active_obj => @state_component_type_tax, :sys_errors => @errors  } %>

    <div class="fields_20_80">
        <%= render :partial => 'state_component_type_taxes/form', :locals => { :f => f } %>
    </div>

    <div class="actions">
        <%= f.submit I18n.translate('view_action.create') %>
    </div>
    
<% end %>

<%= link_to I18n.translate('state_component_type_taxes.index.action'), state_component_type_taxes_path if can? :manage, @state_component_type_tax %>
